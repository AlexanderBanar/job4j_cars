<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        version="2.5">

    <display-name>Cars Ad</display-name>

    <servlet>
        <servlet-name>AuthServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.AuthServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthServlet</servlet-name>
        <url-pattern>/auth.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.RegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegServlet</servlet-name>
        <url-pattern>/reg.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindAllAdsServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.FindAllAdsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindAllAdsServlet</servlet-name>
        <url-pattern>/findAll.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NewAdServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.NewAdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewAdServlet</servlet-name>
        <url-pattern>/newAd.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CurrentDayAdsServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.CurrentDayAdsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CurrentDayAdsServlet</servlet-name>
        <url-pattern>/curAds.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PhotoAdsServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.PhotoAdsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PhotoAdsServlet</servlet-name>
        <url-pattern>/photoAds.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MyAdsServlet</servlet-name>
        <servlet-class>ru.job4j.servlets.MyAdsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyAdsServlet</servlet-name>
        <url-pattern>/myAds.do</url-pattern>
    </servlet-mapping>

</web-app>